{
    "name": "huztw/permission",
    "description": "laravel permission",
    "license": "MIT",
    "authors": [
        {
            "name": "huztw",
            "email": "c0s0c0z0@gmail.com",
            "homepage": "https://github.com/huztw"
        }
    ],
    "homepage": "https://github.com/huztw/permission",
    "keywords": ["Laravel", "Permission"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5|~6",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Huztw\\Permission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Huztw\\Permission\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huztw\\Permission\\PermissionServiceProvider"
            ],
            "aliases": {
                "Permission": "Huztw\\Permission\\Facades\\Permission"
            }
        }
    }
}
